CREATE TABLE `sample_db` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `rukuDate` date NOT NULL COMMENT '上架时间',
  `barCode` varchar(40) COLLATE utf8_bin NOT NULL COMMENT '条码',
  `ord2proId` int(11) NOT NULL COMMENT '关联花型可取的工艺单数据等信息',
  `chengfen` varchar(20) COLLATE utf8_bin NOT NULL COMMENT '成分',
  `chengfenMemo` varchar(40) COLLATE utf8_bin NOT NULL COMMENT '成分详细描述',
  `shazhi` varchar(100) COLLATE utf8_bin NOT NULL COMMENT '纱支',
  `jingwei` varchar(100) COLLATE utf8_bin NOT NULL COMMENT '经纬密',
  `isTanli` int(11) NOT NULL COMMENT '是否弹力',
  `gexing` varchar(40) COLLATE utf8_bin NOT NULL COMMENT '格型',
  `zuzhi` varchar(40) COLLATE utf8_bin NOT NULL COMMENT '组织',
  `hzl` varchar(100) COLLATE utf8_bin NOT NULL COMMENT '后整理工艺',
  `imageFile` varchar(100) COLLATE utf8_bin NOT NULL COMMENT '图片文件',
  `kucunCnt` decimal(10,1) NOT NULL COMMENT '库存数',
  `cangkuCnt` decimal(10,1) NOT NULL COMMENT '仓库数量',
  `kucunWei` varchar(50) COLLATE utf8_bin NOT NULL COMMENT '库存位置',
  `memo` varchar(100) COLLATE utf8_bin NOT NULL COMMENT '备注',
  `isXiajia` smallint(1) NOT NULL DEFAULT '1' COMMENT '是否下架1否0是',
  `cangkuWei` varchar(50) COLLATE utf8_bin NOT NULL COMMENT '仓库位置',
  `factoryCode` varchar(20) COLLATE utf8_bin NOT NULL COMMENT '厂编',
  `proCode` varchar(20) COLLATE utf8_bin NOT NULL COMMENT '产品编号',
  `menfu` varchar(20) COLLATE utf8_bin NOT NULL COMMENT '门幅',
  `creater` varchar(20) COLLATE utf8_bin NOT NULL COMMENT '创建人',
  `dt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `guige` varchar(100) CHARACTER SET utf8 NOT NULL COMMENT '规格',
  `color` varchar(50) CHARACTER SET utf8 NOT NULL COMMENT '颜色',
  `kezhong` varchar(50) CHARACTER SET utf8 NOT NULL COMMENT '克重',
  `ganghao` varchar(20) CHARACTER SET utf8 NOT NULL COMMENT '缸号',
  PRIMARY KEY (`id`),
  KEY `rukuDate` (`rukuDate`),
  KEY `barCode` (`barCode`),
  KEY `ord2proId` (`ord2proId`),
  KEY `chengfen` (`chengfen`),
  KEY `shazhi` (`shazhi`),
  KEY `jingwei` (`jingwei`),
  KEY `isTanli` (`isTanli`),
  KEY `gexing` (`gexing`),
  KEY `zuzhi` (`zuzhi`),
  KEY `hzl` (`hzl`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_bin COMMENT='样品登记表';

